
public class HW extends Assignment{
	private static double hwgradebd;
	private double ind;

	public static double gethwgradebd() {
		return hwgradebd;
	}

	public static void sethwgradebd(double hwgradebd) {
		HW.hwgradebd = hwgradebd;
	}
	
	public double getind() {
		return ind;
	}

	public void setind(double ind) {
		this.ind = ind;
	}

	public HW() {
		super();
		this.ind = 0.0;
	}
	
	public HW(int ptslost, int totalpts, int weight,double ind) {
		super(ptslost,totalpts,weight);
		this.ind = ind;
	}

}
