
public class Graduate extends Student {
	private String underMajor1;
	private String underMajor2;
	private String underMinor;
	private String major;
	
	public String getUnderMajor1() {
		return underMajor1;
	}

	public void setUnderMajor1(String underMajor1) {
		this.underMajor1 = underMajor1;
	}

	public String getUnderMajor2() {
		return underMajor2;
	}

	public void setUnderMajor2(String underMajor2) {
		this.underMajor2 = underMajor2;
	}

	public String getUnderMinor() {
		return underMinor;
	}

	public void setUnderMinor(String underMinor) {
		this.underMinor = underMinor;
	}

	public String getMajor() {
		return major;
	}

	public void setMajor(String major) {
		this.major = major;
	}

	public Graduate() {
		super();
		this.underMajor1 ="None";
		this.underMajor2 ="None";
		this.underMinor ="None";
		this.major ="None";
	}

	public Graduate(String name,String sid,String major,int year,String email,int grade) {
		super(name,sid,major,year,email,grade);
		this.underMajor1 ="None";
		this.underMajor2 ="None";
		this.underMinor ="None";
		this.major ="None";		
	}
	
	public Graduate(String name,String sid,String major,int year,String email,int grade,String major1,String major2,String minor) {
		super(name,sid,major,year,email,grade);
		this.underMajor1 = major1;
		this.underMajor2 = major2;
		this.underMinor = minor;
		this.major = major;		
	}
}
