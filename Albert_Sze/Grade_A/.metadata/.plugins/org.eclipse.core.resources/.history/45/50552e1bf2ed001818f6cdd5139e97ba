import java.sql.*;
import javax.swing.*;

public class SqliteConnection {
	Connection conn =null;
	public static Connection dbConnector() {
		try {
			Class.forName("org.sqlite.JDBC");
			Connection conn = DriverManager.getConnection("jdbc:sqlite:path");
			return conn;			
		}catch(Exception e)
		{
			return null;			
		}
	}

}
